{
 	valid 	 ; valid information
 	la 	 ; Location area indication
 	att 	 ; attach / detach flag
 	re 	 ; re-establishment flag
 	band 	 ; band (dummy)
 	ncc 	 ; national colour code
 	bcc 	 ; base station colour code
 	t3212 	 ; periodic updating timer period
}



