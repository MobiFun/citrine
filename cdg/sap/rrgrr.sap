            <Value ValueType="DEC">7</Value>
            <Version>!REL99 OR !TI_PS_FF_EMR</Version>
            <Comment>Measurement report list size (1 serving and 6 neighbour cells)</Comment>
        </Constant>
        <Constant>
            <Alias>RRGRR_BA_LIST_SIZE</Alias>
