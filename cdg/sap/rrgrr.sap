                <Name>RRGRR_CBCH_INFO_IND</Name>
                <PrimID Direction="UPLINK" Number="28"/>
                <PrimUsage>
                    <Sender>GRR</Sender>
                    <Receiver>RR</Receiver>
                </PrimUsage>
                <Version>REL99</Version>
            </PrimDef>
