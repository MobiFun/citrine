        <PrimBasicElem>
            <Description>
                <DocElement>
                    <XHTMLElement Type="TEXT_SECTION">The channel needed field indicates which type of channel is needed for the mobile station for the transaction linked to the paging procedure. The field is coded according to following table:</XHTMLElement>
                </DocElement>
                <DocElement>
                    <XHTMLElement Type="TEXT_SECTION">0 Any channel</XHTMLElement>
                </DocElement>
                <DocElement>
                    <XHTMLElement Type="TEXT_SECTION">1 SDCCH</XHTMLElement>
                </DocElement>
                <DocElement>
                    <XHTMLElement Type="TEXT_SECTION">2 TCH/F (Full rate)</XHTMLElement>
                </DocElement>
                <DocElement>
                    <XHTMLElement Type="TEXT_SECTION">3 TCH/H or TCH/F (Dual rate)</XHTMLElement>
                </DocElement>
            </Description>
            <PrimBasicElemDef>
                <Name>ch_needed</Name>
                <Type>U8</Type>
                <Comment>Channel needed field</Comment>
            </PrimBasicElemDef>
