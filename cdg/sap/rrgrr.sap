    <PrimitivesSection PrimIDType="BIT16" SAPid="45">
        <Description>
            <DocElement>
                <XHTMLElement Type="TEXT_SECTION"> This section contains all primitives that are defined for the RRGRR SAP</XHTMLElement>
            </DocElement>
        </Description>
        <Primitive>
            <Description>
                <DocElement>
                    <XHTMLElement Type="TEXT_SECTION">This primitive indicates whether GPRS service is supported in the serving cell and/or that the SI13 was received.</XHTMLElement>
                </DocElement>
            </Description>
            <PrimDef>
                <Name>RRGRR_GPRS_SI13_IND</Name>
                <PrimID Direction="DOWNLINK" Number="0"/>
                <PrimUsage>
                    <Sender>RR</Sender>
                    <Receiver>GRR</Receiver>
                </PrimUsage>
            </PrimDef>
            <PrimItem Presentation="MANDATORY">
                <ItemLink>
                    <DocName DocType="SAP">rrgrr</DocName>
                    <Name>cause</Name>
                </ItemLink>
